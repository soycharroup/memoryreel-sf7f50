{
  "name": "memoryreel-backend",
  "version": "1.0.0",
  "description": "MemoryReel backend services - AI-powered digital memory management platform",
  "main": "dist/server.js",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rimraf dist coverage",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "security:audit": "npm audit",
    "docker:build": "docker build -t memoryreel-backend .",
    "docker:run": "docker run -p 3000:3000 memoryreel-backend",
    "db:migrate": "ts-node scripts/migrate.ts",
    "db:seed": "ts-node scripts/seed.ts",
    "generate:docs": "typedoc --out docs src",
    "validate": "npm run lint && npm run format:check && npm run test"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.0.0",
    "@aws-sdk/client-rekognition": "^3.0.0",
    "@google-cloud/vision": "^3.0.0",
    "@stripe/stripe-node": "^12.0.0",
    "aws-sdk": "^2.1.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.0",
    "clamav.js": "^1.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dd-trace": "^3.15.0",
    "dotenv": "^16.3.1",
    "exifr": "^7.1.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "ioredis": "^5.3.2",
    "joi": "^17.9.0",
    "jsonwebtoken": "^9.0.0",
    "limiter": "^5.1.0",
    "mime-types": "^2.1.35",
    "mongoose": "^7.4.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "openai": "^4.0.0",
    "opossum": "^6.0.0",
    "prom-client": "^14.0.0",
    "qrcode": "^1.5.0",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.0.0",
    "retry": "^0.13.1",
    "sharp": "^0.32.0",
    "speakeasy": "^2.0.0",
    "stripe": "^11.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.0",
    "winston-cloudwatch": "^3.1.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "aws-sdk-client-mock": "^3.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "jest-mock": "^29.0.0",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^3.0.0",
    "prettier": "^2.8.0",
    "redis-mock": "^0.56.3",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "author": "MemoryReel Team",
  "license": "UNLICENSED"
}